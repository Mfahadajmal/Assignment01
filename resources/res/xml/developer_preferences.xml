<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" android:key="@string/pref_category_developer_key">
    <com.google.android.accessibility.utils.preference.AccessibilitySuitePreference
        android:title="@string/title_pref_version_code"
        android:selectable="false"
        android:key="@string/pref_developer_version_code_key"/>
    <com.google.android.accessibility.utils.preference.AccessibilitySuiteSwitchPreference
        android:title="@string/title_pref_diagnosis_mode"
        android:key="@string/pref_diagnosis_mode_key"
        android:defaultValue="@bool/pref_diagnosis_mode_default"/>
    <com.google.android.accessibility.utils.preference.AccessibilitySuiteSwitchPreference
        android:title="@string/title_pref_tts_overlay"
        android:key="@string/pref_tts_overlay_key"
        android:defaultValue="@bool/pref_tts_overlay_default"/>
    <com.google.android.accessibility.utils.preference.AccessibilitySuiteSwitchPreference
        android:title="@string/title_pref_talkback_gesture_detection"
        android:key="@string/pref_talkback_gesture_detection_key"
        android:defaultValue="@bool/pref_talkback_gesture_detection_default"/>
    <com.google.android.accessibility.utils.preference.AccessibilitySuiteSwitchPreference
        android:title="@string/title_pref_multiple_gesture_set"
        android:key="@string/pref_multiple_gesture_set_key"
        android:defaultValue="@bool/pref_multiple_gesture_set_default"/>
    <com.google.android.accessibility.utils.preference.AccessibilitySuiteSwitchPreference
        android:title="@string/title_pref_echo_recognized_text_speech"
        android:key="@string/pref_echo_recognized_text_speech_key"
        android:defaultValue="@bool/pref_echo_recognized_text_default"/>
    <com.google.android.accessibility.utils.preference.AccessibilitySuiteSwitchPreference
        android:title="@string/title_pref_performance_stats"
        android:key="@string/pref_performance_stats_reflect_key"
        android:defaultValue="@bool/pref_performance_stats_default"/>
    <com.google.android.accessibility.utils.preference.AccessibilitySuiteSwitchPreference
        android:title="@string/title_pref_explore_by_touch"
        android:key="@string/pref_explore_by_touch_reflect_key"
        android:defaultValue="@bool/pref_explore_by_touch_default"/>
    <com.google.android.accessibility.utils.preference.AccessibilitySuiteSwitchPreference
        android:title="@string/title_pref_tree_debug"
        android:key="@string/pref_tree_debug_reflect_key"
        android:defaultValue="@bool/pref_tree_debug_default"/>
    <com.google.android.accessibility.utils.preference.AccessibilitySuiteSwitchPreference
        android:title="@string/title_pref_log_overlay"
        android:key="@string/pref_log_overlay_key"
        android:defaultValue="@bool/pref_log_overlay_default"/>
    <com.google.android.accessibility.utils.preference.AccessibilitySuiteListPreference
        android:entries="@array/pref_log_level_entries"
        android:title="@string/title_pref_log_level"
        android:key="@string/pref_log_level_key"
        android:summary="%s"
        android:defaultValue="@string/pref_log_level_default"
        android:dialogTitle="@string/title_pref_log_level"
        android:entryValues="@array/pref_log_level_values"/>
    <com.google.android.accessibility.utils.preference.AccessibilitySuitePreference
        android:title="@string/title_pref_dump_a11y_event"
        android:key="@string/pref_dump_a11y_event_key"
        android:fragment="com.google.android.accessibility.talkback.preference.base.TalkBackDumpAccessibilityEventFragment"/>
</PreferenceScreen>
