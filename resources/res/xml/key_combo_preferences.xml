<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" android:key="@string/pref_category_manage_gestures_key">
    <PreferenceCategory
        android:title="@string/keycombo_menu_category_select_keymap"
        android:key="@string/pref_keymap_category_key">
        <ListPreference
            android:entries="@array/pref_select_keymap_entries"
            android:title="@string/keycombo_menu_select_keymap"
            android:key="@string/pref_select_keymap_key"
            android:summary="%s"
            android:defaultValue="@string/default_keymap_entry_value"
            android:entryValues="@array/pref_select_keymap_values"/>
        <PreferenceScreen
            android:title="@string/keycombo_menu_reset_keymap"
            android:key="@string/pref_reset_keymap_key"/>
    </PreferenceCategory>
    <PreferenceCategory
        android:title="@string/keycombo_menu_category_navigation"
        android:key="@string/pref_navigation_actions_category_key">
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_navigate_next"
            android:key="@string/keycombo_shortcut_navigate_next_default"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_navigate_previous"
            android:key="@string/keycombo_shortcut_navigate_previous_default"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_navigate_first"
            android:key="@string/keycombo_shortcut_navigate_first"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_navigate_last"
            android:key="@string/keycombo_shortcut_navigate_last"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_perform_click"
            android:key="@string/keycombo_shortcut_perform_click"/>
    </PreferenceCategory>
    <PreferenceCategory
        android:title="@string/keycombo_menu_category_global"
        android:key="@string/pref_global_actions_category_key">
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_global_back"
            android:key="@string/keycombo_shortcut_global_back"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_global_home"
            android:key="@string/keycombo_shortcut_global_home"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_global_recent"
            android:key="@string/keycombo_shortcut_global_recents"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_global_notifications"
            android:key="@string/keycombo_shortcut_global_notifications"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_global_play_pause_media"
            android:key="@string/keycombo_shortcut_global_play_pause_media"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_global_scroll_forward_reading_menu"
            android:key="@string/keycombo_shortcut_global_scroll_forward_reading_menu"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_global_scroll_backward_reading_menu"
            android:key="@string/keycombo_shortcut_global_scroll_backward_reading_menu"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_global_adjust_reading_settings_previous"
            android:key="@string/keycombo_shortcut_global_adjust_reading_settings_previous"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_global_adjust_reading_setting_next"
            android:key="@string/keycombo_shortcut_global_adjust_reading_setting_next"/>
    </PreferenceCategory>
    <PreferenceCategory
        android:title="@string/keycombo_menu_category_granularity"
        android:key="@string/pref_navigation_category_key">
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_granularity_increase"
            android:key="@string/keycombo_shortcut_granularity_increase"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_granularity_decrease"
            android:key="@string/keycombo_shortcut_granularity_decrease"/>
    </PreferenceCategory>
    <PreferenceCategory
        android:title="@string/keycombo_menu_category_other"
        android:key="@string/pref_other_actions_category_key">
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_other_read_from_top"
            android:key="@string/keycombo_shortcut_other_read_from_top"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_other_read_from_cursor_item"
            android:key="@string/keycombo_shortcut_other_read_from_cursor_item"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_other_toggle_search"
            android:key="@string/keycombo_shortcut_other_toggle_search"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_other_talkback_context_menu"
            android:key="@string/keycombo_shortcut_other_talkback_context_menu"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_other_custom_actions"
            android:key="@string/keycombo_shortcut_other_custom_actions"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_other_language_options"
            android:key="@string/keycombo_shortcut_other_language_options"/>
        <com.google.android.accessibility.talkback.preference.base.KeyboardShortcutDialogPreference
            android:title="@string/keycombo_menu_other_copy_last_spoken_phrase"
            android:key="@string/keycombo_shortcut_other_copy_last_spoken_phrase"/>
    </PreferenceCategory>
</PreferenceScreen>
